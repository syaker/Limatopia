    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.8.0",
    "concurrently": "5.2.0",
    "eslint": "^6.0.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.1.2",
    "firebase-mock": "2.3.2",
    "gh-pages": "3.0.0",
